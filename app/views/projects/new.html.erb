<h1>Create a project</h1>
<%= form_for @project do |f| %>
  <%= f.label :title %><br />
  <%= f.text_field :title %>

  <%= f.label :description %><br />
  <%= f.text_area :description %>

  <%= f.label :goal %><br />
  <%= f.number_field :goal %>

  <%= f.label :end_date %><br />
  <%= f.date_select :end_date %>

  <%= f.submit %>
<% end %>